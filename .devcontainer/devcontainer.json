{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "monosans.djlint",
        "samuelcolvin.jinjahtml",
        "ms-python.python",
        "VisualStudioExptTeam.vscodeintellicode"
      ],
      "settings": {
        "editor.fontFamily": "ui-monospace, SFMono-Regular, \"SF Mono\", Menlo, Consolas, \"Liberation Mono\", monospace",
        "editor.renderWhitespace": "boundary"
      }
    }
  }
}
